{
    "name": "olafnorge/oauth-proxy",
    "description": "Application to proxy oauth requests to a provider.",
    "keywords": ["oauth", "proxy"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Volker Machon",
            "email": "olafnorge@users.noreply.github.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.olafnorge.de/storage/repositories-from-githubcom"
        },
        {
            "type": "composer",
            "url": "https://satis.olafnorge.de/storage/repository-from-gitlabolafnorgede"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "doctrine/dbal": "^2.8",
        "illuminate/redis": "^5.6",
        "laravel/lumen-framework": "5.6.*",
        "laravel/socialite": "3.0.x-dev",
        "olafnorge/docker-helpers": "^1.0",
        "olafnorge/laravel-migrate": "^1.0",
        "olafnorge/laravel-rollbar": "^2.1",
        "symfony/lock": "^4.1",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "^2.2",
        "flipbox/lumen-generator": "^5.6",
        "fzaninotto/faker": "~1.4",
        "laravel/homestead": "^7.15",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^7.3",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-autoload-dump": [
            "@php artisan ide-helper:generate 1>/dev/null 2>&1 || true",
            "@php artisan ide-helper:meta 1>/dev/null 2>&1 || true"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
